{
	"name": "dmzx/partner",
	"type": "phpbb-extension",
	"description": "This extension will add a partner page to your forum, where you can list your partners with a description and a logo.",
	"homepage": "http://www.dmzx-web.net",
	"version": "1.0.0",
	"time": "2015-10-25",
	"keywords": ["phpbb", "extension", "partner"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "http://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Partner Page",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0,<3.2.*@dev"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
			"directory": "/versions",
			"filename": "partner_version.json"
		}
	}
}
